{
    "name": "badta5te/gendiff",
    "description": "Utility to find differences between 2 files",
    "type": "project",
    "keywords": [
        "cli-utility",
        "train project",
        "diff files"
    ],
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "files": [
            "src/Cli.php",
            "src/Parser.php",
            "src/Engine.php"
        ],
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "docopt/docopt": "^1.0",
        "funct/funct": "^1.5",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "phpunit/phpunit": "^8"
    },
    "authors": [
        {
            "name": "Pavel",
            "email": "mikhailov.sztu@gmail.com"
        }
    ],
    "scripts": {
        "phpcs": "phpcs",
        "phpunit" :"phpunit"
    }
}
